package com.one;

class Employee1 {
	int id;
	String name;
	double salary;
	String designation;
	String department;
	
	public Employee1 () {
		System.out.println("Name: "+name+ " ID: "+id+ " Salary: "+salary+
				" Designation: "+designation+ " Department: "+department);
	}
}


public class ConstructorEmployee {
	public static void main(String[] args) {
		Employee emp = new Employee();
		
	}
}
